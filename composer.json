{
    "name": "hammerspacecouk/cargo-api",
    "description": "API for the application",
    "type": "project",
    "autoload": {
        "psr-4": { "App\\": "src" },
        "files": [
            "src/Functions/Classes.php",
            "src/Functions/DateTimes.php",
            "src/Functions/Numbers.php",
            "src/Functions/Strings.php",
            "src/Functions/Transforms.php"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\App\\": "tests/" }
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-sodium": "*",
        "abraham/twitteroauth": "^0.7.4",
        "aws/aws-sdk-php": "^3.44",
        "doctrine/doctrine-bundle": "^1.8",
        "facebook/graph-sdk": "^5.6",
        "google/apiclient": "^2.1",
        "paragonie/paseto": "^1.0",
        "psr/cache": "^1.0",
        "ramsey/uuid": "^3.6",
        "ramsey/uuid-doctrine": "^1.4",
        "roave/doctrine-simplecache": "^2.1",
        "stevenmaguire/oauth2-microsoft": "^2.2",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phploc/phploc": "^4.0",
        "phpstan/phpstan": "^0.9",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.2",
        "symfony/dotenv": "^4.0",
        "symfony/profiler-pack": "^1.0"
    },
    "authors": [
        {
            "name": "Hammerspace.co.uk",
            "email": "contact@hammerspace.co.uk"
        }
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": "phpunit",
        "loc": "phploc public src tests bin",
        "cbf": "phpcbf public src tests bin",
        "cs": "phpcs public src tests bin",
        "stan": "phpstan analyse public src tests bin --level 7 -c phpstan.neon"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0MH09ZEX89TV51BQ273G78J",
            "allow-contrib": false
        }
    }
}
