{
    "name": "hammerspacecouk/cargo-api",
    "description": "API for the application",
    "type": "project",
    "autoload": {
        "psr-4": { "App\\": "src" },
        "classmap": [ "app/Kernel.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\App\\": "tests/" }
    },
    "require": {
        "php": "^7.1",
        "symfony/symfony": "^3.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/swiftmailer-bundle": "^3.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "google/apiclient": "^2.1",
        "lcobucci/jwt": "^3.2",
        "psr/cache": "^1.0",
        "ramsey/uuid": "^3.6",
        "ramsey/uuid-doctrine": "^1.3",
        "roave/doctrine-simplecache": "^1.1",
        "facebook/graph-sdk": "^5.6",
        "abraham/twitteroauth": "^0.7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/dotenv": "^3.3",
        "phpstan/phpstan": "^0.8.0"
    },
    "authors": [
        {
            "name": "Hammerspace.co.uk",
            "email": "contact@hammerspace.co.uk"
        }
    ],
    "scripts": {
        "test": "phpunit",
        "cbf": "phpcbf app src tests",
        "cs": "phpcs app src tests",
        "stan": "phpstan analyse src tests --level 7 -c phpstan.neon"
    }
}
