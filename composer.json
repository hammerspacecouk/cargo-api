{
    "name": "hammerspacecouk/cargo-api",
    "description": "API for the application",
    "type": "project",
    "autoload": {
        "psr-4": { "App\\": "src" },
        "classmap": [ "app/Kernel.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\App\\": "tests/" }
    },
    "require": {
        "php": "^7.1",
        "symfony/symfony": "^3.3",
        "symfony/monolog-bundle": "^3.1",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "psr/cache": "^1.0",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "beberlei/DoctrineExtensions": "^1.0",
        "ramsey/uuid": "^3.6",
        "ramsey/uuid-doctrine": "^1.3",
        "lcobucci/jwt": "^3.2",
        "google/apiclient": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/dotenv": "^3.3"
    },
    "authors": [
        {
            "name": "Hammerspace.co.uk",
            "email": "contact@hammerspace.co.uk"
        }
    ],
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs app src tests"
    }
}
